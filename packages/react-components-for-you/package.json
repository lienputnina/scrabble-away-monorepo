{
  "name": "@liene-putnina/react-components-for-you",
  "version": "1.6.0",
  "description": "Storybook-based React component library",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lienputnina/react-components-for-you.git"
  },
  "keywords": [
    "storybook",
    "react",
    "component",
    "library"
  ],
  "author": "Liene Putni≈Üa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lienputnina/react-components-for-you.git/issues"
  },
  "homepage": "https://github.com/lienputnina/react-components-for-you.git#readme",
  "scripts": {
    "prepare": "husky install",
    "start": "pnpm storybook",
    "lint": "pnpm lint:eslint && pnpm lint:ts",
    "lint:eslint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:ts": "tsc",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md,mdx}\"",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage",
    "storybook": "start-storybook -p 6006 --ci",
    "copy-styles": "mkdir -p dist/styles/scss && cp -r src/styles/scss dist/styles",
    "build": "rm -rf dist && pnpm build:library && pnpm build:types && pnpm copy-styles",
    "build:library": "rollup -c",
    "build:types": "tsc --project tsconfig.build.json",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "classnames": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@liene-putnina/eslint-config-lintmyride": "workspace:*",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-image": "^3.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-viewport": "^6.5.12",
    "@storybook/builder-webpack4": "^6.5.9",
    "@storybook/manager-webpack4": "^6.5.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/dom": "^8.16.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.3.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "css-loader": "5.2.7",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.1",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rollup": "^2.77.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.53.0",
    "sass-loader": "10.1.1",
    "storybook-css-modules-preset": "^1.1.1",
    "storybook-dark-mode": "^1.1.0",
    "style-loader": "2.0.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": "18.x.x",
    "react-dom": "18.x.x"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md,mdx}": "pnpm format"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
